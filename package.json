{
  "name": "scl",
  "version": "0.1.0",
  "description": "",
  "main": "src/app.jsx",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --hot --colors --content-base static/",
    "build": "echo 'File Size (before): ' && ls -lah dist/scl.min.js | awk '{ print $5}' && NODE_ENV=production webpack -p --config webpack.production.config.js --progress --colors",
    "test": "npm run lint",
    "lint": "./node_modules/eslint/bin/eslint.js -c ./.eslintrc --ext .jsx --ext .js src",
    "stats": "webpack --profile --json > stats.json && echo 'Upload here ---> webpack.github.io/analyse/#modules'",
    "clean": "rm -rf node_modules/ && npm i && npm start"
  },
  "author": "justin@secretcowlevel.com",
  "license": "none",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.3.26",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.2.3",
    "isomorphic-fetch": "^2.2.1",
    "react": "^0.14.7",
    "react-bootstrap": "^0.28.1",
    "react-dom": "^0.14.7",
    "react-hot-loader": "^1.2.8",
    "react-redux": "^4.4.1",
    "redux": "^3.3.1",
    "redux-devtools": "^3.1.1",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "webpack": "^1.10.5",
    "webpack-dev-server": "^1.10.1"
  }
}
