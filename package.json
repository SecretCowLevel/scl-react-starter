{
    "name": "scl",
    "version": "0.2.0",
    "description": "",
    "main": "src/app.jsx",
    "scripts": {
        "start": "NODE_ENV=development webpack-dev-server --hot --colors --content-base static/",
        "build": "echo 'File Size (before): ' && ls -lah dist/scl.min.js | awk '{ print $5}' && NODE_ENV=production webpack -p --config webpack.production.config.js --progress --colors",
        "test": "npm run lint",
        "lint": "./node_modules/eslint/bin/eslint.js -c ./.eslintrc --ext .jsx --ext .js src",
        "stats": "webpack --profile --json > stats.json && echo 'Upload here ---> webpack.github.io/analyse/#modules'",
        "clean": "rm -rf node_modules/ && npm i && npm start"
    },
    "author": "justin@secretcowlevel.com",
    "license": "none",
    "dependencies": {},
    "devDependencies": {
        "babel-core": "^6.3.26",
        "babel-eslint": "^6.0.4",
        "babel-loader": "^6.2.0",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-polyfill": "^6.7.2",
        "babel-preset-es2015": "^6.3.13",
        "babel-preset-react": "^6.3.13",
        "babel-preset-stage-0": "^6.5.0",
        "compression-webpack-plugin": "^0.3.1",
        "eslint": "^2.4.0",
        "eslint-config-airbnb": "^8.0.0",
        "eslint-plugin-import": "^1.6.1",
        "eslint-plugin-jsx-a11y": "^1.0.4",
        "eslint-plugin-react": "^5.0.1",
        "isomorphic-fetch": "^2.2.1",
        "react": "^15.0.2",
        "react-bootstrap": "^0.29.3",
        "react-dom": "^15.0.2",
        "react-hot-loader": "^1.2.8",
        "react-redux": "^4.4.1",
        "redux": "^3.3.1",
        "redux-devtools": "^3.1.1",
        "redux-logger": "^2.6.1",
        "redux-thunk": "^2.0.1",
        "webpack": "^1.10.5",
        "webpack-dev-server": "^1.10.1"
    }
